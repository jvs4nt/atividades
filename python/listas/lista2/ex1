lista_numeros= []

for i in range(10):
    lista_numeros.append(int(input("digite um numero da lista")))

maior = lista_numeros[0]
indice = 0

for i in range(1, 11):
    if (lista_numeros[i] > maior):
        maior = lista_numeros[i]
        indice = i

print(f"o maior elemento Ã©: {maior} | indice: {indice}")